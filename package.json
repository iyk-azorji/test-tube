{
  "name": "test-tube",
  "version": "1.0.0",
  "description": "React + Redux boilerplate with hot reloading and babel support.",
  "main": "index.js",
  "repository": "git@github.com:iyk-azorji/test-tube.git",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save"
  },
  "author": "iyk-azorji",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.0.2",
    "chai-jquery": "^2.0.0",
    "cheerio": "^0.22.0",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-watch": "^3.1.0",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.20.2",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "jquery": "^3.2.1",
    "jsdom": "^11.0.0",
    "mocha": "^3.4.2",
    "nock": "^9.0.13",
    "npm-run-all": "^4.0.2",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "^1.3.1",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "axios": "*",
    "babel-preset-stage-1": "*",
    "lodash": "*",
    "react": "*",
    "react-dom": "*",
    "react-redux": "*",
    "react-router": "*",
